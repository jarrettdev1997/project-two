<table id="board-game" data-gameid="{{game.id}}">
    <tr>

        <td id="upper_left" cell = "0">
            <span class="material-icons">{{getXorO game.board.upper_left}}</span>
        </td>
        <td id="upper_mid" cell = "1">
            <span class="material-icons">{{getXorO game.board.upper_mid}}</span>
        </td>
        <td id="upper_right" cell = "2">
            <span class="material-icons">{{getXorO game.board.upper_right}}</span>
        </td>
    </tr>
    <tr>
        <td id="center_left" cell = "3">
            <span class="material-icons">{{getXorO game.board.center_left}}</span>
        </td>
        <td id="center_mid" cell = "4">
            <span class="material-icons">{{getXorO game.board.center_mid}}</span>
        </td>
        <td id="center_right" cell = "5">
            <span class="material-icons">{{getXorO game.board.center_right}}</span>
        </td>
    </tr>
    <tr>
        <td id="lower_left" cell = "6">
            <span class="material-icons">{{getXorO game.board.lower_left}}</span>
        </td>
        <td id="lower_mid" cell = "7">
            <span class="material-icons">{{getXorO game.board.lower_mid}}</span>
        </td>
        <td id="lower_right" cell = "8">
            <span class="material-icons">{{getXorO game.board.lower_right}}</span>
        </td>
    </tr>
</table>